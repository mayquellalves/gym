<?xml version="1.0" encoding="UTF-8"?>
<xml-fragment xmlns:env="http://www.bea.com/wli/config/env" xmlns:ema="http://www.bea.com/wli/sb/transports/email">
  <aler:description xmlns:aler="http://www.bea.com/wli/monitoring/alert">Alert para o log dos servicos referentes a Partner / Problem</aler:description>
  <aler:AlertToConsole xmlns:aler="http://www.bea.com/wli/monitoring/alert">true</aler:AlertToConsole>
  <aler:AlertToReportingDataSet xmlns:aler="http://www.bea.com/wli/monitoring/alert">false</aler:AlertToReportingDataSet>
  <aler:AlertToSNMP xmlns:aler="http://www.bea.com/wli/monitoring/alert">true</aler:AlertToSNMP>
  <aler:Destinations type="email" xmlns:aler="http://www.bea.com/wli/monitoring/alert">
    <aler:config xsi:type="ema1:EmailActionProviderConfiguration" xmlns:ema1="http://www.bea.com/wli/monitoring/alert/action/email" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
      <ema1:transport-end-point-config>
        <ema1:config xsi:type="tran:EndPointConfiguration" xmlns:tran="http://www.bea.com/wli/sb/transports">
          <tran:provider-id>email</tran:provider-id>
          <tran:inbound>false</tran:inbound>
          <tran:URI>
            <env:value>mailto:email_qqr@netservicos.com.br</env:value>
          </tran:URI>
          <tran:provider-specific>
            <ema:outbound-properties>
              <ema:smtp-resource ref="System/SMTP Servers/NETSMTPServer"/>
              <ema:connection-timeout>0</ema:connection-timeout>
              <ema:timeout>0</ema:timeout>
              <ema:from-address>monitoracao.soa@netservicos.com.br</ema:from-address>
              <ema:from-name>Monitoracao SOA</ema:from-name>
              <ema:reply-to-address/>
              <ema:reply-to-name/>
              <ema:mail-session-jndiname/>
            </ema:outbound-properties>
            <ema:request-encoding>iso-8859-1</ema:request-encoding>
          </tran:provider-specific>
        </ema1:config>
      </ema1:transport-end-point-config>
    </aler:config>
  </aler:Destinations>
</xml-fragment>